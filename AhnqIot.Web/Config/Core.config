<?xml version="1.0" encoding="utf-8"?>
<!--核心设置-->
<Setting>
  <!--全局调试。XTrace.Debug-->
  <Debug>false</Debug>
  <!--日志等级，只输出大于等于该级别的日志-->
  <LogLevel>Info</LogLevel>
  <!--文本日志目录-->
  <LogPath>../Log</LogPath>
  <!--临时目录-->
  <TempPath>XTemp</TempPath>
  <!--扩展插件存放目录-->
  <PluginPath>Bin</PluginPath>
  <!--扩展插件服务器。将从该网页上根据关键字分析链接并下载插件-->
  <PluginServer>http://www.newlifex.com/showtopic-51.aspx</PluginServer>
  <!--下载扩展插件的缓存目录。默认位于系统盘的X\Cache-->
  <PluginCache>C:\X\Cache</PluginCache>
  <!--网络调试-->
  <NetDebug>false</NetDebug>
  <!--多线程调试-->
  <ThreadDebug>false</ThreadDebug>
  <!--网页压缩文件-->
  <WebCompressFiles>.aspx,.axd,.js,.css</WebCompressFiles>
</Setting>